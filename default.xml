<?xml version="1.0" encoding="UTF-8"?>
<manifest>
 <!-- remote servers -->
 <remote name="github-parrot" fetch="https://github.com/Parrot-Developers/"/>
 <remote name="github-akaaba" fetch="https://github.com/Akaaba/"/>
 <remote name="gerrit" fetch="ssh://gerrit-doc.parrot.biz" review="gerrit-doc.parrot.biz"/>
 <default remote="gerrit" revision="master" sync-j="4"/>

 <!-- build system -->
 <project name="build/Alchemy" path="build/alchemy"/>
 <project name="drones/dragon_build" path="build/dragon_build">
    <linkfile src="root-build.sh" dest="build.sh"/>
    <linkfile src="root-track-branch.sh" dest="track-branch.sh"/>
 </project>
 <project name="drones/apps_tools" path="build/apps_tools"/>

 <!-- external -->
 <project name="aftermarket/ARDroneSDK3/ARSDKTools" path="packages/ARSDKTools"/>

 <!-- ARSDK -->
 <project name="aftermarket/ARDroneSDK3/ARSDKBuildUtils" path="packages/ARSDKBuildUtils"/>
 <project name="aftermarket/ARDroneSDK3/ARSDK3" path="packages/ARSDK3"/>
 <project name="aftermarket/ARDroneSDK3/libARCommands" path="packages/libARCommands"/>
 <project name="aftermarket/ARDroneSDK3/libARController" path="packages/libARController"/>
 <project name="aftermarket/ARDroneSDK3/libARDataTransfer" path="packages/libARDataTransfer"/>
 <project name="aftermarket/ARDroneSDK3/libARDiscovery" path="packages/libARDiscovery"/>
 <project name="aftermarket/ARDroneSDK3/libARMavlink" path="packages/libARMavlink"/>
 <project name="aftermarket/ARDroneSDK3/libARMedia" path="packages/libARMedia"/>
 <project name="aftermarket/ARDroneSDK3/libARNetwork" path="packages/libARNetwork"/>
 <project name="aftermarket/ARDroneSDK3/libARNetworkAL" path="packages/libARNetworkAL" />
 <project name="aftermarket/ARDroneSDK3/libARSAL" path="packages/libARSAL"/>
 <project name="aftermarket/ARDroneSDK3/libARStream" path="packages/libARStream" />
 <project name="aftermarket/ARDroneSDK3/libARStream2" path="packages/libARStream2" />
 <project name="aftermarket/ARDroneSDK3/libARUpdater" path="packages/libARUpdater"/>
 <project name="aftermarket/ARDroneSDK3/libARUtils" path="packages/libARUtils"/>
 <project name="drones/arsdk-xml" path="packages/arsdk-xml"/>

 <!-- other packages (alphabetically sorted by name) -->
 <project name="drones/libfutils" path="packages/libfutils"/>
 <project name="drones/libh264" path="packages/libh264"/>
 <project name="drones/libmp4" path="packages/libmp4"/>
 <project name="drones/libmux" path="packages/libmux"/>
 <project name="drones/librtp" path="packages/librtp"/>
 <project name="drones/libvideo-metadata" path="packages/libvideo-metadata"/>
 <project name="drones/libvideo-streaming" path="packages/libvideo-streaming"/>
 <project name="librtsp" remote="github-akaaba" path="packages/librtsp"/>
 <project name="libsdp" remote="github-akaaba" path="packages/libsdp"/>
 <project name="pdraw" remote="github-akaaba" path="packages/pdraw">
  <linkfile src="config/pclinux" dest="products/pdraw/linux/config"/>
  <linkfile src="config/raspi" dest="products/pdraw/raspi/config"/>
  <linkfile src="config/android" dest="products/pdraw/android/config"/>
  <linkfile src="config/raspi" dest="products/pdraw/raspi/config"/>
  <linkfile src="config/buildcfg.py" dest="products/pdraw/buildcfg.py"/>
 </project>
 <project name="pdraw_prebuilt" remote="github-akaaba" path="packages/prebuilt"/>
 <project name="pulsar/libpomp" path="packages/libpomp"/>
 <project name="pulsar/ulog" path="packages/ulog"/>
 <project name="videocore4-ilclient" remote="github-akaaba" path="packages/ilclient"/>

</manifest>
